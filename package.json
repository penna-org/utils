{
  "name": "@pennajs/utils",
  "type": "module",
  "version": "0.0.4-beta.1",
  "packageManager": "pnpm@9.1.1",
  "author": {
    "email": "aibayanyu@qq.com",
    "name": "aibayanyu20",
    "url": "https://yanyu.me"
  },
  "license": "Apache-2.0",
  "keywords": [
    "utils",
    "penna",
    "vue"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./lodash": {
      "types": "./dist/lodash.d.ts",
      "import": "./dist/lodash.js",
      "require": "./dist/lodash.cjs",
      "default": "./dist/lodash.js"
    },
    "./dom/*": {
      "types": "./dist/dom/*.d.ts",
      "import": "./dist/dom/*.js",
      "require": "./dist/dom/*.cjs",
      "default": "./dist/dom/*.js"
    },
    "./vue/*": {
        "types": "./dist/vue/*.d.ts",
        "import": "./dist/vue/*.js",
        "require": "./dist/vue/*.cjs",
        "default": "./dist/vue/*.js"
    },
    "./*": {
        "types": ["./dist/*.d.ts","./dist/*/*.d.ts"],
        "import": ["./dist/*.js","./dist/*/*.js"],
        "require": ["./dist/*.cjs","./dist/*/*.cjs"],
        "default": ["./dist/*.js","./dist/*/*.js"]
    },
    "./dist/*": {
        "types": ["./dist/*.d.ts","./dist/*/*.d.ts"],
        "import": ["./dist/*.js","./dist/*/*.js"],
        "require": ["./dist/*.cjs","./dist/*/*.cjs"],
        "default": ["./dist/*.js","./dist/*/*.js"]
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint src --fix",
    "release": "bumpp && pnpm publish",
    "prepublish": "pnpm build"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.0",
    "bumpp": "^9.4.1",
    "eslint": "^8.57.0",
    "lodash-es": "^4.17.21",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "vue": "^3.4.27"
  }
}
